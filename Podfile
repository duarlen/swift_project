# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'SwiftProject' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SwiftProject
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'SnapKit'
  pod 'RealmSwift'

  # 布局框架
  pod 'SnapKit'
  
  # 图片缓存框架
  pod 'Kingfisher'
  
  # 网络框架和数据解析
  pod 'Moya/RxSwift'
  pod 'HandyJSON'
  
  # 动画
  pod 'libpag'      # pag 动效播放器
  pod 'lottie-ios'  # lottie 动效播放器
  pod 'SVGAPlayer'  # svga 动效播放器
  pod 'QGVAPlayer'  # mp4 动效播放器
  
end
